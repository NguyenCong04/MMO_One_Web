        margin-bottom: 20px;
    }

    #upload .icon {
        display: flex;
        align-items: center;
        justify-content: center;
    }

    #upload .icon svg {
        height: 80px;
        fill: rgba(75, 85, 99, 1);
    }

    #upload .text {
        display: flex;
        align-items: center;
        justify-content: center;
    }

    #upload .text span {
